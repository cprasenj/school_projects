doctype html
html
 head
  title Students
  link(rel='stylesheet', href='/stylesheets/style.css')  
 body
  a(href="/") Home
  h1 Students  
  p
   each grade in grades
    div
     a(href="/grade/#{grade.id}") #{grade.name}
     ul
      - console.log(grades, grade);
      each student in grade.students
       li
        a(href="/student/#{student.id}") #{student.name}